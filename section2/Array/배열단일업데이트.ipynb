    "def solution(n,A,i,j,k):\n",
    "    for idx in range(i,j+1):\n",
    "        A[idx] = A[idx]*k\n",
    "    return sum(A)\n",
    "\n",
    "n = (int,input())\n",
    "A = list(map(int,input().split()))\n",
    "i,j,k = map(int,input().split())\n",
    "\n",
    "print(solution(n,A,i,j,k))\n"
   ]
