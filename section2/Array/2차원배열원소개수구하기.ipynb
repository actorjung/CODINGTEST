   "source": [
    "def solution(n,A,k):\n",
    "    answer = 0\n",
    "    for i in range(n):\n",
    "        for j in range(n):\n",
    "            if A[i][j] == k:\n",
    "                answer += 1\n",
    "    return answer\n",
    "\n",
    "n,k = map(int,input().split())\n",
    "A = list(list(map(int,input().split())) for _ in range(n))\n",
    "print(solution(n,A,k))"
   ]
